  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.21.5",
    "nodemon": "^2.0.4",
    "pg": "^8.3.3",
    "postgrator-cli": "^3.2.0"
  },
  "devDependencies": {
    "babel-jest": "^26.3.0",
    "supertest": "^5.0.0"
  }
}
