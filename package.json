{
  "name": "recipe-server",
  "version": "1.0.0",
  "description": "Recipe Finder server",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
